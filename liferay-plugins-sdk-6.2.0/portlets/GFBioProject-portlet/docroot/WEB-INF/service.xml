<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="org.gfbio">
	<author>Marcel Froemming</author>
	<namespace>gfbio</namespace>



	<!-- Head, Column & Content Tables -->



	<!-- Head -->

	<entity name="Head" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="headID" 			type="long" 		primary="true" />
		<column name="table_name" 		type="String" />
		<column name="table_type" 		type="String" />

		<order by="asc">				<order-column name="table_name" />	</order>

		<finder name="HeadId" 		return-type="head">					<finder-column name="headID" />					</finder>
		<finder name="TableName" 	return-type="head">					<finder-column name="table_name" />				</finder>
		<finder name="TableType"	return-type="Collection">			<finder-column name="table_type" />				</finder>
	</entity>



	<!-- Column -->

	<entity name="Column" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="columnID"			type="long" 		primary="true" />
		<column name="headID" 			type="long" />
		<column name="column_name" 		type="String" />

		<order by="asc">	<order-column name="columnID" />	</order>

		<finder name="ColumnId" 			return-type="Column">		<finder-column name="columnID" />												</finder>
		<finder name="ColumnName" 			return-type="Colection">	<finder-column name="column_name" />											</finder>
		<finder name="HeadId"				return-type="Collection">	<finder-column name="headID" />													</finder>
		<finder name="HeadIdAndColumnName"	return-type="Collection">	<finder-column name="headID" />			<finder-column name="column_name" />	</finder>		
	</entity>



	<!-- Content -->

	<entity name="Content" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="contentID" 			type="long" 		primary="true" />
		<column name="headID" 				type="long" />
		<column name="columnID" 			type="long" />
		<column name="rowID" 				type="long" />
		<column name="cellContent" 			type="String" />

		<order by="asc">	<order-column name="contentID" />	</order>

		<finder name="ContentId" 			return-type="Content">			<finder-column name="contentID" />											</finder>
		<finder name="TableIds" 			return-type="Content">			<finder-column name="columnID" />	<finder-column name="rowID" />			</finder>
		<finder name="HeadId" 				return-type="Collection">		<finder-column name="headID" />												</finder>
		<finder name="ColumnId"				return-type="Collection">		<finder-column name="columnID" />											</finder>
		<finder name="RowId"				return-type="Collection">		<finder-column name="rowID" />												</finder>
		<finder name="CellContent" 			return-type="Collection">		<finder-column name="cellContent" />										</finder>
		<finder name="CellContentOfColumn"	return-type="Collection">		<finder-column name="cellContent" />	<finder-column name="columnID" />	</finder>
	</entity>
	
	
	
	<!-- Other Entities -->



	<!-- Basket   -->

	<entity name="Basket" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="basketID" 			type="long" 	primary="true" />
		<column name="userID" 				type="long" />
		<column name="name" 				type="String" />
		<column name="lastModifiedDate" 	type="Date" />
		<column name="basketJSON" 			type="String" />
		<column name="queryJSON" 			type="String" />

		<order by="desc">		<order-column name="lastModifiedDate" />		</order>

		<finder name="UserId" return-type="Collection">			<finder-column name="userID" />																		</finder>
		<finder name="UserIdSince" return-type="Collection">	<finder-column name="userID" />		<finder-column name="lastModifiedDate"  comparator="&gt;" />	</finder>
		<finder name="BasketId" return-type="Basket">			<finder-column name="basketID" />																	</finder>
		<finder name="BasketIds" return-type="Collection">		<finder-column name="basketID"  arrayable-operator="OR" />											</finder>
	</entity>
	
	
	
	<!-- Data Provider -->

	<entity name="DataProvider" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="dataProviderID" 		type="long" 		primary="true" />
		<column name="name" 				type="String" />
		<column name="label" 				type="String" />
		<column name="description" 			type="String" />
		<column name="address" 				type="String" />
		<column name="website" 				type="String" />
		<column name="training" 			type="String" />

		<order by="asc">	<order-column name="name" />	</order>

		<finder name="Name" 			return-type="Collection">				<finder-column name="name" />				</finder>
		<finder name="Label" 			return-type="DataProvider">				<finder-column name="label" />				</finder>
		<finder name="DataProviderID" 	return-type="DataProvider">				<finder-column name="dataProviderID" />		</finder>
	</entity>
	
	

	<!-- Project -->

	<entity name="Project" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="projectID" 			type="long" 		primary="true" />
		<column name="parentProjectID" 		type="long" />
		<column name="name" 				type="String" />
		<column name="label" 				type="String" />
		<column name="description" 			type="String" />
		<column name="startDate" 			type="Date" />
		<column name="endDate" 				type="Date" />
		<column name="status" 				type="String" />

		<order by="asc">	<order-column name="name" />	</order>

		<finder name="Name" 			return-type="Collection">				<finder-column name="name" />				</finder>
		<finder name="Label" 			return-type="Collection">				<finder-column name="label" />				</finder>
		<finder name="ProjectID" 		return-type="Project">					<finder-column name="projectID" />			</finder>
		<finder name="ParentProjectID" 	return-type="Project">					<finder-column name="parentProjectID" />	</finder>
	</entity>



	<!-- Research Object  -->

	<entity name="ResearchObject" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="researchObjectID" 		type="long" 		primary="true" />
		<column name="version" 					type="int"  />
		<column name="parentResearchObjectID" 	type="long"  />
		<column name="name" 					type="String" />
		<column name="label" 					type="String" />
		<column name="metadata"					type="String" />
		<column name="formatmetadata"			type="String" />
		<column name="researchObjectType"		type="String" />

		<order by="asc">		<order-column name="name" />		</order>

		<finder name="Name" 					return-type="Collection">		<finder-column name="name" />						</finder>
		<finder name="Label" 					return-type="Collection">		<finder-column name="label" />						</finder>
		<finder name="ResearchObjectID"			return-type="ResearchObject">	<finder-column name="researchObjectID" />			</finder>
		<finder name="ParentID"					return-type="Collection">		<finder-column name="parentResearchObjectID" />		</finder>
	</entity>

	
	
	<!-- Submission Registry  -->

	<entity name="SubmissionRegistry" data-source="gfbioDataSource" uuid="false"  local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="researchObjectID" 		type="long" 			primary="true" />
		<column name="researchObjectVersion" 	type="long"				primary="true" />
		<column name="archive"			 		type="String"			primary="true" />
		<column name="brokerSubmissionID" 		type="String"	 />
		<column name="archivePID" 				type="String"	 />
		<column name="archivePIDType" 			type="long"	 />
		<column name="lastChanged" 				type="Date"	 />
		<column name="userID" 					type="long"	 />
		<column name="isPublic"	 				type="Boolean"	 />
		<column name="publicAfter" 				type="Date"	 />		
		<column name="status" 					type="String"	 />		

		<order by="asc">				<order-column name="researchObjectID" />		</order>

		<finder name="ResearchObjectID"						return-type="Collection">			<finder-column name="researchObjectID" />														</finder>
		<finder name="ResearchObjectIDWithVersion" 			return-type="Collection">			<finder-column name="researchObjectID" />		<finder-column name="researchObjectVersion" />	</finder>
		<finder name="ResearchObjectIDWithLastChanges" 		return-type="SubmissionRegistry">	<finder-column name="researchObjectID" />		<finder-column name="lastChanged" />			</finder>
		<finder name="Archive"								return-type="Collection">			<finder-column name="archive" />																</finder>
		<finder name="BrockerSubmissionID"					return-type="Collection">			<finder-column name="brokerSubmissionID" />													</finder>
		<finder name="BrockerSubmissionIDWithLastChanges"	return-type="SubmissionRegistry">	<finder-column name="brokerSubmissionID" />	<finder-column name="lastChanged" />			</finder>
		<finder name="PersistentIdentifierID"				return-type="Collection">			<finder-column name="archivePID" />																</finder>
		<finder name="PersistentIdentifierTypeID"			return-type="Collection">			<finder-column name="archivePIDType" />															</finder>
		<finder name="UserID"								return-type="Collection">			<finder-column name="userID" />																	</finder>
		<finder name="IsPublic"								return-type="Collection">			<finder-column name="isPublic" />																</finder>
		<finder name="Status"								return-type="Collection">			<finder-column name="status" />																	</finder>
	</entity>

	
	
	<!-- User Extension  -->

	<entity name="UserExtension" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="userID" 	type="long" 					primary="true" />

		<order 	by="asc">															<order-column name="userID" />		</order>

		<finder name="UserID" 	return-type="UserExtension">						<finder-column name="userID" />		</finder>
	</entity>
	
	

	<!-- Other Relationships -->



	<!-- Relationship between Data Provider and Persistent Identifier (in Content)    -->

	<entity name="DataProvider_PersistentIdentifier" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="gfbioDataProvider" 			type="long" 	db-name="gfbio_dataprovider" 			primary="true" />
		<column name="gfbioPersistentIdentifier" 	type="long" 	db-name="gfbio_persistentidentifier"	primary="true" />

		<finder name="DataProvider" 			return-type="Collection">	<finder-column name="gfbioDataProvider" />				</finder>
		<finder name="PersistentIdentifier"		return-type="Collection">	<finder-column name="gfbioPersistentIdentifier" />		</finder>
	</entity>
	
	
	
	<!-- Relationship between Project and ResearchObject  -->

	<entity name="Project_ResearchObject" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="projectID" 			type="long" 	primary="true" />
		<column name="researchObjectID" 	type="long" 	primary="true" />

		<finder name="ProjectID" 			return-type="Collection">		<finder-column name="projectID" />			</finder>
		<finder name="ResearchObjectID"		return-type="Collection">		<finder-column name="researchObjectID" />	</finder>
	</entity>



	<!-- Relationship between Project and User as "PI of"    -->

	<entity name="Project_User_PI" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="projectID" 			type="long" 	primary="true" />
		<column name="userID" 				type="long" 	primary="true" />
		<column name="startDate" 			type="Date" />
		<column name="endDate" 				type="Date" />

		<finder name="ProjectID" 		return-type="Collection">			<finder-column name="projectID" />			</finder>
		<finder name="UserID"	 		return-type="Collection">			<finder-column name="userID" />				</finder>
	</entity>



	<!-- Relationship between Project and User as "works in"   -->

	<entity name="Project_User" data-source="gfbioDataSource" local-service="true" remote-service="true" session-factory="gfbioSessionFactory" tx-manager="gfbioTransactionManager" cache-enabled="false">
		<column name="projectID"			type="long"		primary="true" />
		<column name="userID" 				type="long" 	primary="true" />
		<column name="startDate" 			type="Date" />
		<column name="endDate" 				type="Date" />

		<finder name="ProjectID"		return-type="Collection">			<finder-column name="projectID" />												</finder>
		<finder name="UserID" 			return-type="Collection">			<finder-column name="userID" />													</finder>
		<finder name="findByPrimaryKey" return-type="Project_User_Worker">	<finder-column	name="projectID" />			<finder-column	name="userID" />	</finder>
	</entity>


</service-builder>